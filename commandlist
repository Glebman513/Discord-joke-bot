import discord
import requests
import random
import json

com_req = ("/список команд", "/команды", "/Команды")

async def commandes(message):
  if message.content.startswith(com_req):
        await message.channel.send('1)/анек - команда, по которой бот выдаёт один из анекдотов в его базе, список которых постоянно пополняется\n2)/сколько анеков - бот выдаст количество анекдотов, которые есть сейчас в его базе\n3)/пономеру (число) - бот выдаст анекдот с этим номером из списка\n4)/рулетка - игра в рулетку, подробные правила можно вызвать командой "/правила рулетки"')
